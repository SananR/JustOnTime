{
  "compilerOptions": {
    "allowJs": false,
    "noEmit": true,
    "target": "es5",
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "cypress",
      "node"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "exclude": [
    "cypress/e2e/**/*.spec.js"
  ],
  "files": [
    "empty.ts"
  ],
  "include": [
    "src"
  ]
}
